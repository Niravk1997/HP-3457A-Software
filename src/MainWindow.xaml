                        <Button  Grid.Column="3" BorderThickness="0" Margin="5,2,5,2" Click="LINE_Query_Button_Click">
                            <StackPanel Orientation="Horizontal">
                                <Label Content="LINE?" FontSize="16" Margin="0,-2,0,0"/>
                            </StackPanel>
                        </Button>
                    </Grid>
                </TabItem>
                <TabItem Header="ACAL">
                    <Grid>
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="*"/>
                            <ColumnDefinition Width="*"/>
                            <ColumnDefinition Width="*"/>
                            <ColumnDefinition Width="*"/>
                        </Grid.ColumnDefinitions>
                        <Grid.RowDefinitions>
                            <RowDefinition Height="*"/>
                        </Grid.RowDefinitions>
                        <Button  Grid.Column="0" BorderThickness="0" Margin="5,2,5,2" Click="ACAL_AC_Button_Click">
                            <StackPanel Orientation="Horizontal">
                                <Label Content="ACAL AC" FontSize="16" Margin="0,-2,0,0"/>
                            </StackPanel>
                        </Button>
                        <Button  Grid.Column="1" BorderThickness="0" Margin="5,2,5,2" Click="ACAL_OHMS_Button_Click">
                            <StackPanel Orientation="Horizontal">
                                <Label Content="ACAL OHMS" FontSize="16" Margin="0,-2,0,0"/>
                            </StackPanel>
                        </Button>
                        <Button  Grid.Column="2" BorderThickness="0" Margin="5,2,5,2" Click="ACBAND_SLOW_Query_Button_Click">
                            <StackPanel Orientation="Horizontal">
                                <Label Content="ACBAND SLOW" FontSize="16" Margin="0,-2,0,0"/>
                            </StackPanel>
                        </Button>
                        <Button  Grid.Column="3" BorderThickness="0" Margin="5,2,5,2" Click="ACBAND_FAST_Query_Button_Click">
                            <StackPanel Orientation="Horizontal">
                                <Label Content="ACBAND FAST" FontSize="16" Margin="0,-2,0,0"/>
                            </StackPanel>
                        </Button>
                    </Grid>
                </TabItem>
                <TabItem Header="Config">
                    <Grid>
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="290"/>
                            <ColumnDefinition Width="*"/>
                        </Grid.ColumnDefinitions>
                        <Grid.RowDefinitions>
                            <RowDefinition Height="20"/>
                            <RowDefinition Height="*"/>
                        </Grid.RowDefinitions>
                        <StackPanel Grid.Column="0" Grid.Row="0" Orientation="Horizontal">
                            <Label Content="NPLC 100 Delay (7½ Digit): " Padding="0" VerticalContentAlignment="Center"/>
                            <TextBox x:Name="NPLC_100_Delay_Time_Text" Text="12" VerticalContentAlignment="Center" MinWidth="60" MaxWidth="60"/>
                            <Label Content="Sec" Margin="5,0,0,0" Padding="0" VerticalContentAlignment="Center"/>
                            <Button x:Name="NPLC_100_Time_Button" Content="Set" Margin="10,0,0,0" Padding="10,0,10,0" BorderThickness="0" Click="NPLC_100_Time_Button_Click"/>
                        </StackPanel>
                        <StackPanel Grid.Column="1" Grid.Row="0" Orientation="Horizontal">
                            <Label Content="Command: " Padding="0" Margin="5,0,0,0" VerticalContentAlignment="Center"/>
                            <TextBox x:Name="Send_SCPI_Command_Text" Text="" VerticalContentAlignment="Center" MinWidth="100" MaxWidth="100"/>
                            <Button x:Name="Send_SCPI_Command_Button" Content="Send" Margin="10,0,0,0" Padding="10,0,10,0" BorderThickness="0" Click="Send_SCPI_Command_Button_Click"/>
                            <Button x:Name="Query_SCPI_Command_Button" Content="Query" Margin="10,0,0,0" Padding="10,0,10,0" BorderThickness="0" Click="Query_SCPI_Command_Button_Click"/>
                        </StackPanel>
                    </Grid>
                </TabItem>
            </TabControl>
        </GroupBox>
        <Grid Grid.Row="5" Grid.Column="0" Margin="5,0,5,0">
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="*"/>
                <ColumnDefinition Width="215"/>
            </Grid.ColumnDefinitions>
            <Grid.RowDefinitions>
                <RowDefinition Height="*"/>
            </Grid.RowDefinitions>
            <GroupBox x:Name="Speech_Box" Grid.Row="0" Grid.Column="0">
                <GroupBox.Header>
                    <TextBlock FontWeight="DemiBold" Margin="0,0,0,0">Speech Synthesizer</TextBlock>
                </GroupBox.Header>
                <TabControl>
                    <TabItem Header="Continuous" IsSelected="False">
                        <StackPanel Orientation="Horizontal" VerticalAlignment="Top">
                            <Label Content="Voice Measurement every"/>
                            <TextBox x:Name="Speech_Continuous_Value" Text="" MinWidth="35" MaxWidth="35" Height="22" VerticalContentAlignment="Center"/>
                            <Label Content="minutes."/>
                            <Border x:Name="Speech_Continuous_Set_Border" BorderThickness="2" BorderBrush="White" Margin="5,0,0,0">
                                <Button x:Name="Speech_Continuous_Set_Button" Content="Set" Width="45" BorderThickness="0" Click="Speech_Continuous_Set_Button_Click"/>
                            </Border>
                            <Border x:Name="Speech_Continuous_Clear_Border" BorderThickness="2" BorderBrush="White" Margin="5,0,0,0">
                                <Button x:Name="Speech_Continuous_Clear_Button" Content="Clear" Width="45" BorderThickness="0" Click="Speech_Continuous_Clear_Button_Click"/>
                            </Border>
                        </StackPanel>
                    </TabItem>
                    <TabItem Header="Min" IsSelected="False">
                        <StackPanel Orientation="Horizontal" VerticalAlignment="Top">
                            <Label Content="Voice when Measurement &lt;"/>
                            <TextBox x:Name="Speech_MIN_Value" Text="" MinWidth="80" MaxWidth="80" Height="22" VerticalContentAlignment="Center"/>
                            <Border x:Name="Speech_MIN_Border" BorderThickness="2" BorderBrush="White" Margin="5,0,0,0">
                                <Button x:Name="Speech_MIN_Button" Content="Set" Width="45" BorderThickness="0" Click="Speech_MIN_Button_Click"/>
                            </Border>
                            <Border x:Name="Speech_MIN_Clear_Border" BorderThickness="2" BorderBrush="White" Margin="5,0,0,0">
                                <Button x:Name="Speech_MIN_Clear_Button" Content="Clear" Width="45" BorderThickness="0" Click="Speech_MIN_Clear_Button_Click"/>
                            </Border>
                        </StackPanel>
                    </TabItem>
                    <TabItem Header="Max" IsSelected="False">
                        <StackPanel Orientation="Horizontal" VerticalAlignment="Top">
                            <Label Content="Voice when Measurement &gt;"/>
                            <TextBox x:Name="Speech_MAX_Value" Text="" MinWidth="80" MaxWidth="80" Height="22" VerticalContentAlignment="Center"/>
                            <Border x:Name="Speech_MAX_Border" BorderThickness="2" BorderBrush="White" Margin="5,0,0,0">
                                <Button x:Name="Speech_MAX_Button" Content="Set" Width="45" BorderThickness="0" Click="Speech_MAX_Button_Click"/>
                            </Border>
                            <Border x:Name="Speech_MAX_Clear_Border" BorderThickness="2" BorderBrush="White" Margin="5,0,0,0">
                                <Button x:Name="Speech_MAX_Clear_Button" Content="Clear" Width="45" BorderThickness="0" Click="Speech_MAX_Clear_Button_Click"/>
                            </Border>
                        </StackPanel>
                    </TabItem>
                </TabControl>
            </GroupBox>
            <GroupBox x:Name="UpdateSpeed_Box" Grid.Row="0" Grid.Column="1" IsEnabled="False">
                <GroupBox.Header>
                    <TextBlock FontWeight="DemiBold" Margin="0,0,0,0">Update Speed</TextBlock>
                </GroupBox.Header>
                <Grid>
                    <Grid.RowDefinitions>
                        <RowDefinition Height="*"/>
                        <RowDefinition Height="*"/>
                    </Grid.RowDefinitions>
                    <StackPanel Orientation="Horizontal" VerticalAlignment="Top" Grid.Row="0" Grid.Column="0">
                        <Label Content="Update Rate (sec):"/>
                        <TextBox x:Name="UpdateSpeed_Value" Text="1" MinWidth="50" MaxWidth="50" Height="22" VerticalContentAlignment="Center"/>
                        <Border x:Name="UpdateSpeed_Value_Set_Border" BorderThickness="2" BorderBrush="White" Margin="3,0,0,0">
                            <Button x:Name="UpdateSpeed_Value_Set_Button" Content="Set" Width="35" BorderThickness="0" Click="UpdateSpeed_Value_Set_Button_Click"/>
                        </Border>
                    </StackPanel>
                    <StackPanel Orientation="Horizontal" VerticalAlignment="Top" Grid.Row="1" Grid.Column="0">
                        <Border x:Name="UpdateSpeed_Default_Set_Border" BorderThickness="2" BorderBrush="#00CE30" Margin="5,0,0,0">
                            <Button x:Name="UpdateSpeed_Default_Set_Button" Content="Set Default" Width="90"  Height="25" BorderThickness="0" Click="UpdateSpeed_Default_Set_Button_Click"/>
                        </Border>
                        <Border x:Name="UpdateSpeed_Fast_Set_Border" BorderThickness="2" BorderBrush="White" Margin="5,0,0,0">
                            <Button x:Name="UpdateSpeed_Fast_Set_Button" Content="Set Fast" Width="90"  Height="25" BorderThickness="0" Click="UpdateSpeed_Fast_Set_Button_Click"/>
                        </Border>
                    </StackPanel>
                </Grid>
            </GroupBox>
        </Grid>
        <GroupBox Grid.Row="6" Grid.Column="0" Margin="5,0,5,0">
            <GroupBox.Header>
                <StackPanel Orientation="Horizontal">
                    <Button Content="Clear" Margin="0,0,5,0" Width="32" Height="19" BorderThickness="0" Click="ClearOutputLog_Click"/>
                    <TextBlock FontWeight="Bold" Margin="0,2,0,0">Output Log</TextBlock>
                </StackPanel>
            </GroupBox.Header>
            <ScrollViewer x:Name="Output_Log_Scroll" Margin="0,0,0,0" HorizontalScrollBarVisibility="Auto">
                <TextBlock x:Name="Output_Log" FontSize="12"/>
            </ScrollViewer>
        </GroupBox>
        <StatusBar Grid.Row="7" Grid.Column="0">
            <StackPanel Orientation="Horizontal" Height="22">
                <Label Margin="0,-4,0,1" Content="Runtime: "/>
                <Label x:Name="Runtime_Timer" Margin="-8,-3,0,1" Content="00:00:00"/>
                <Rectangle Margin="0,0,0,0" Width="1" Height="23" Fill="Gray"/>
                <Label Margin="0,-4,0,1" Content="Speech:"/>
                <Label x:Name="Speech_Status" Margin="-8,-3,0,1" Content="Disabled"/>
                <Rectangle Margin="0,0,0,0" Width="1" Height="23" Fill="Gray"/>
                <Label Margin="0,-4,0,1" Content="Total Samples:"/>
                <Label x:Name="Samples_Total" Margin="-5,-3,0,1" Content="0"/>
                <Rectangle Margin="0,0,0,0" Width="1" Height="23" Fill="Gray"/>
                <Label Margin="0,-3,0,1" Content="Invalid/Timeout: "/>
                <Label x:Name="InvalidSamples_Total" Margin="-8,-3,0,1" Content="0"/>
            </StackPanel>
        </StatusBar>
    </Grid>
</Window>
